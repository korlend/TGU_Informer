<resources>
    <string name="app_name">TGU Informer</string>
    <string name="action_settings">Settings</string>
    <string name="appbar_scrolling_view_behavior">Scroll</string>

    <string name="day_spinner_group_title">Выберите группу или преподавателя</string>
    <string name="day_spinner_faculty_title">Выберите факультет</string>

    <!-- заголовки для пунктов параметров меню -->
    <string name="menu_title_lecturer_mode">Я преподаватель</string>
    <string name="menu_title_student_mode">Я студент</string>
    <string name="menu_title_sync">Обновить</string>
    <string name="menu_title_preferences">Настройки</string>

    <!-- время начала и окончания каждой из шести пар -->
    <string name="first_pair_start">8:30</string>
    <string name="first_pair_end">10:00</string>
    <string name="second_pair_start">10:15</string>
    <string name="second_pair_end">11:45</string>
    <string name="third_pair_start">12:45</string>
    <string name="third_pair_end">14:15</string>
    <string name="fourth_pair_start">14:30</string>
    <string name="fourth_pair_end">16:00</string>
    <string name="fifth_pair_start">16:15</string>
    <string name="fifth_pair_end">17:45</string>
    <string name="sixth_pair_start">18:00</string>
    <string name="sixth_pair_end">19:30</string>
    <string name="seventh_pair_start">19:45</string>
    <string name="seventh_pair_end">21:15</string>

    <!-- ключи для хранения информации о занятиях -->
    <string name="map_name_lesson_discipline">discipline</string>
    <string name="map_name_lesson_lecturer">lecturer</string>
    <string name="map_name_lesson_auditory">auditory</string>
    <string name="map_name_lesson_kind_of_work">kindofwork</string>
    <string name="map_name_lesson_pair_number">pair_number</string>
    <string name="map_name_lesson_id">id</string>
    <string name="map_name_lesson_group_abbr">group</string>
    <string name="map_name_lesson_date">date</string>

    <string-array name="map_name_lesson_array">
        <item>@string/map_name_lesson_discipline</item>
        <item>@string/map_name_lesson_lecturer</item>
        <item>@string/map_name_lesson_auditory</item>
        <item>@string/map_name_lesson_kind_of_work</item>
        <item>@string/map_name_lesson_pair_number</item>
        <item>@string/map_name_lesson_id</item>
        <item>@string/map_name_lesson_group_abbr</item>
    </string-array>

    <string name="map_name_group_name">name</string>
    <string name="map_name_group_course">course</string>
    <string name="map_name_group_faculty">faculty</string>

    <string-array name="map_name_group_array">
        <item>@string/map_name_group_name</item>
        <item>@string/map_name_group_course</item>
        <item>@string/map_name_group_faculty</item>
    </string-array>

    <string name="map_name_faculty_oid">oid</string>
    <string name="map_name_faculty_name">name</string>
    <string name="map_name_faculty_abbr">abbr</string>

    <string-array name="map_name_faculty_array">
        <item>@string/map_name_faculty_oid</item>
        <item>@string/map_name_faculty_name</item>
        <item>@string/map_name_faculty_abbr</item>
    </string-array>

    <!--
    json параметры для разбора значений ответа сервера
    напротив каждого параметра указан тип передаваемого значения
     -->
    <string name="parse_server_response_lesson_discipline">disciplineName</string>             <!-- String -->
    <string name="parse_server_response_lesson_lecturer">lecturerFIO</string>                  <!-- String -->
    <string name="parse_server_response_lesson_auditory">auditoriumAbbr</string>               <!-- String -->
    <string name="parse_server_response_lesson_kind_of_work">kindOfWorkName</string>           <!-- String -->
    <string name="parse_server_response_lesson_pair_number">numberOfPair</string>              <!-- String -->
    <string name="parse_server_response_lesson_id">oid</string>                                <!-- Integer -->
    <string name="parse_server_response_lesson_start_time">startOn</string>                    <!-- String -->
    <string name="parse_server_response_lesson_end_time">endOn</string>                        <!-- String -->
    <string name="parse_server_response_lesson_modified_time">modifiedTime</string>            <!-- String -->
    <string name="parse_server_response_lesson_group_abbr">groupAbbr</string>                  <!-- String -->

    <string-array name="parse_server_response_lesson_array">
        <item>@string/parse_server_response_lesson_discipline</item>
        <item>@string/parse_server_response_lesson_lecturer</item>
        <item>@string/parse_server_response_lesson_auditory</item>
        <item>@string/parse_server_response_lesson_kind_of_work</item>
        <item>@string/parse_server_response_lesson_pair_number</item>
        <item>@string/parse_server_response_lesson_id</item>
        <item>@string/parse_server_response_lesson_group_abbr</item>
    </string-array>

    <string name="parse_server_response_group_name">name</string>                              <!-- String -->
    <string name="parse_server_response_group_course">course</string>                          <!-- Integer -->
    <string name="parse_server_response_group_faculty">faculty</string>                        <!-- Integer -->

    <string-array name="parse_server_response_group_array">
        <item>@string/parse_server_response_group_name</item>
        <item>@string/parse_server_response_group_course</item>
        <item>@string/parse_server_response_group_faculty</item>
    </string-array>

    <string name="parse_server_response_faculty_oid">oid</string>                                  <!-- Integer -->
    <string name="parse_server_response_faculty_name">name</string>                                <!-- String -->
    <string name="parse_server_response_faculty_abbr">abbr</string>                                <!-- String -->

    <string-array name="parse_server_response_faculty_array">
        <item>@string/parse_server_response_faculty_oid</item>
        <item>@string/parse_server_response_faculty_name</item>
        <item>@string/parse_server_response_faculty_abbr</item>
    </string-array>

    <!-- имена для хранения пользовательских настроек -->
    <string name="response_pref_name">last_response</string>
    <string name="preferences_name">TGU_Informer_Prefs</string>

    <!-- используемый формат даты в приложении -->
    <string name="date_format_to_server_request">yyyy-MM-dd</string>

    <!--
    url адреса для запросов к серверу
    перед каждым url описываются передаваемые значения
    -->
    <!-- %1$s : группа, %2$s : дата запрашиваемого расписания
    <string name="url_server_data_get_by_day">http://10.0.2.2:8090/data/get/mysql/schedule/%1$s/%2$s</string>-->
    <!-- %1$s : группа, %2$s : дата, с которой происходит выборка, %3$s : дата, по которую происходит выборка -->
    <string name="url_server_data_get_by_date_range">http://10.0.2.2:8090/data/get/mysql/student/schedule/%1$s/%2$s/%3$s</string>
    <!-- запрос для получения списка факультетов -->
    <string name="url_server_data_get_all_faculties">http://10.0.2.2:8090/data/GetAllFacultiesMySQL</string>
    <!-- запрос для получения списка групп -->
    <string name="url_server_data_get_all_groups">http://10.0.2.2:8090/data/GetAllGroupsMySQL</string>
    <!-- запрос для получения списка преподавателей -->
    <string name="url_server_data_get_all_lecturers">http://10.0.2.2:8090/data/GetAllLecturersMySQL</string>
    <!-- %1$s : преподаватель, %2$s : дата, с которой происходит выборка, %3$s : дата, по которую происходит выборка -->
    <string name="url_server_data_get_lecturer_schedule_by_date_range">http://10.0.2.2:8090/data/get/mysql/lecturer/schedule/%1$s/%2$s/%3$s</string>
</resources>
